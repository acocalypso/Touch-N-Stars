    "baseline-browser-mapping": "2.9.19",
    "cross-env": "10.1.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-vue": "^10.8.0",
    "lodash": "4.17.23",
    "prettier": "3.8.1",
    "raw-loader": "^4.0.2",
    "vue-eslint-parser": "^10.4.0"
  }
