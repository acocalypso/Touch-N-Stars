{
  "name": "touch-n-stars",
  "version": "4.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "serve": "npm run lint:eslint && node scripts/generate-plugin-registry.js && node scripts/generate-whats-new.js && vue-cli-service serve",
    "build": "npm run lint:eslint && npm run build:app",
    "build:app": "node scripts/generate-plugin-registry.js && node scripts/generate-whats-new.js && vue-cli-service build",
    "build:native": "npm run lint:eslint && cross-env EXCLUDE_STELLARIUM_DATA=true npm run build:app",
    "lint": "vue-cli-service lint",
    "lint:eslint": "eslint . --ext .js,.vue --fix",
    "lint:fix": "eslint . --ext .js,.vue --fix",
    "testbuild": "node scripts/generate-plugin-registry.js && node scripts/generate-whats-new.js && npm run lint:eslint && vue-cli-service build --no-lint --dest \"%LOCALAPPDATA%\\NINA\\Plugins\\3.0.0\\Touch 'N' Stars\\app\"",
    "format": "prettier --write \"src/**/*.{js,vue}\"",
    "ionic:build": "npm run lint:eslint  && node scripts/generate-whats-new.js && npm run build:native",
    "ionic:serve": "npm run serve",
    "locale:entry": "node scripts/add-locale-entry.js",
    "locale:entry:all": "node scripts/add-locale-entry-all.js",
    "generate-plugins": "node scripts/generate-plugin-registry.js",
    "generate-whats-new": "node scripts/generate-whats-new.js",
    "create-plugin": "node scripts/generate-plugin.js"
  },
  "dependencies": {
    "@capacitor-community/keep-awake": "8.0.0",
