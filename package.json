    "chart.js": "4.5.1",
    "core-js": "3.48.0",
    "date-fns": "4.1.0",
    "file-saver": "^2.0.5",
    "i18n": "0.15.3",
    "jszip": "^3.10.1",
    "papaparse": "5.5.3",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "4.7.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "uuid": "13.0.0",
    "vue": "3.5.28",
    "vue-draggable-next": "^2.2.1",
    "vue-i18n": "11.2.8",
