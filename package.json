    "@capacitor/ios": "8.1.0",
    "@capacitor/keyboard": "^8.0.0",
    "@capacitor/status-bar": "8.0.1",
    "@capawesome/capacitor-android-edge-to-edge-support": "8.0.5",
    "@capawesome/capacitor-file-picker": "8.0.1",
    "@capgo/capacitor-updater": "8.43.3",
    "@devioarts/capacitor-mdns": "^0.0.2",
    "@heroicons/vue": "^2.2.0",
    "@microsoft/signalr": "^10.0.0",
    "@panzoom/panzoom": "^4.6.1",
    "@pinkcao/vue-drag-resize-rotate": "^2.0.0",
